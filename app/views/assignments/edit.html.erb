<%= form_for(@assignment) do |form| %>
    <label> Title</label><br>
    <%= form.text_field :title %> <br>
    <label>Start End</label><br>
    <%= form.date_field :start_date %> <br>
    <label> Description</label><br>
    <%= form.text_field :todo %> <br>


    <%= form.submit "Save", {class: "btn btn-success"} %><br><br>

    <%= link_to "Back", :back %>
    <% end %>